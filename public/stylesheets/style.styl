font-size = 14px
stylefordiv(w,l,r)
  width:w
  margin-left:l
  margin-right:r
border_style = 1px solid red

*
  padding:0
  margin:0
  border:0
.clear
  clear:both
  height:0
  line-height:0
  font-size:0
body
  padding-top: 50px
  font: font-size "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF
  text-decoration:none
#header
  height:50px
  stylefordiv 80% auto auto
  font: font-size - 2px
  background:white
  border:border_style
  padding:0px
#footer
  stylefordiv 80% auto auto
  font: font-size - 4px
  text-align:right
  height:30px
  border:border_style

#shell
  stylefordiv 80% auto auto
  height:1000px
  font:font-size
  text-align:left
  background:black
  color:white
  border:border_style
  overflow-y:scroll
ul#nav 
  list-style:none
  width:100%  
  margin:0px
#nav li 
  width:20%
  text-align:center
  float:left
  margin-right:13%
